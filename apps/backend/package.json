    "luxon": "^3.4.4",
    "magic-bytes.js": "^1.10.0",
    "muhammara": "^4.1.0",
    "multer": "^1.4.4",
    "openapi-typescript-codegen": "^0.29.0",
    "openid-client": "^5.6.4",
    "pg": "^8.11.5",
    "pg-large-object": "^2.0.0",
    "prom-client": "^15.1.3",
